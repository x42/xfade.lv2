@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pg:    <http://lv2plug.in/ns/ext/port-groups#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mod: <http://moddevices.com/ns/mod#> .

<http://gareus.org/rgareus#me>
	a foaf:Person ;
	foaf:name "Robin Gareus" ;
	foaf:mbox <mailto:robin@gareus.org> ;
	foaf:homepage <http://gareus.org/> .

<http://gareus.org/oss/lv2/xfade>
	a lv2:Plugin, lv2:MixerPlugin, doap:Project;
	doap:license <http://usefulinc.com/doap/licenses/gpl> ;
	doap:maintainer <http://gareus.org/rgareus#me> ;
	doap:name "Stereo X-Fade";
	@VERSION@
	mod:brand "x42" ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 0 ;
		lv2:symbol "xfade" ;
		lv2:name "Signal A/B";
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
		lv2:maximum 1.0 ;
		lv2:scalePoint [
			rdfs:label "A" ;
			rdf:value -1.0
		] , [
			rdfs:label "A+B" ;
			rdf:value 0.0
		] , [
			rdfs:label "B" ;
			rdf:value 1.0
		]
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 1 ;
		lv2:symbol "shape" ;
		lv2:name "Shape";
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
		lv2:scalePoint [
			rdfs:label "Linear" ;
			rdf:value 0.0
		] , [
			rdfs:label "Equal Power" ;
			rdf:value 1.0
		]
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "mode" ;
		lv2:name "Mode";
		lv2:default 0 ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
		lv2:portProperty lv2:integer, lv2:enumeration;
		lv2:scalePoint [
			rdfs:label "X-fade: Overlap [-1..+1]" ;
			rdf:value 0 ;
		] , [
			rdfs:label "V-fade: Non-overlap [-1..0][0..1]" ;
			rdf:value 1 ;
		]
	] , [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 3 ;
		lv2:symbol "inA_left" ;
		lv2:name "In A Left" ;
		lv2:designation pg:left ;
	] , [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 4 ;
		lv2:symbol "inA_right" ;
		lv2:name "In A Right" ;
		lv2:designation pg:right ;
	] , [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 5 ;
		lv2:symbol "inB_left" ;
		lv2:name "In B Left" ;
		lv2:designation pg:left ;
	] , [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 6 ;
		lv2:symbol "inB_right" ;
		lv2:name "In B Right" ;
		lv2:designation pg:right ;
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 7 ;
		lv2:symbol "out_left" ;
		lv2:name "Out Left" ;
		lv2:designation pg:left ;
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 8 ;
		lv2:symbol "out_right" ;
		lv2:name "Out Right" ;
		lv2:designation pg:right ;
	] ;
	rdfs:comment "A stereo crossfade plugin"
	.
